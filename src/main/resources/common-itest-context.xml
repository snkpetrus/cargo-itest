<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.springframework.org/schema/aop
       http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
       http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">

	<bean id="abstractContainerUtil" class="nl.tranquilizedquality.itest.cargo.AbstractJBossContainerUtil" abstract="true">
		<property name="cargoLogFilePath" value="target/" />
		<property name="containerPort" value="8890" />
		<property name="jvmArguments">
			<list>
				<value>-Xms128m</value>
				<value>-Xmx512m</value>
				<value>-XX:PermSize=128m</value>
			</list>
		</property>
	</bean>
</beans>
